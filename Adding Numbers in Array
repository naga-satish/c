#include<stdio.h>
#include<conio.h>
void main()
{
int sum=0,n,j,i,a[10000],r;
printf("enter N:");
scanf("%d",&n);
for(j=0;j<n;j++)
{
    r=j+1;
    printf("enter %d element : ",r);
    scanf("%d",&a[j]);
}
for(i=0;i<n;i++)
{
sum=(sum+a[i]);
}
printf("sum of entered %d elements is 1%d ",n,sum);
getch();
}
